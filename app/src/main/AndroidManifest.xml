<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.juegoecologico">

    <application
        android:allowBackup="true"
        android:icon="@drawable/mundo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/mundo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".AboutActivity"></activity>
        <activity android:name=".InstruccionesActivity" />
        <activity android:name=".ResultadoActivity" />
        <activity android:name=".JuegoActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>